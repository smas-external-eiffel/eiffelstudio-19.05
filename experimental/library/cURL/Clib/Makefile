SHELL = /bin/sh
AR = ar rc
CC = gcc
CTAGS = ctags
INCLUDE = -I/home/eiffel/deliv/PorterPackage/Eiffel_19.05/studio/spec/linux-x86-64/include -I../spec/include -I../../../C_library/curl/include
CFLAGS = -O3 -pipe -fPIC -D_GNU_SOURCE -m64  -g $(INCLUDE)
MTCFLAGS = -O3 -pipe -fPIC -D_GNU_SOURCE -m64 -DEIF_THREADS -DEIF_LINUXTHREADS  -g $(INCLUDE)
SHAREDLINK = ld
LDSHAREDFLAGS = -m elf_x86_64 -shared -o
LN = /bin/ln
MAKE = make
MKDEP = \$(EIFFEL_SRC)/C/mkdep $(DPFLAGS) --
RANLIB = :
RM = /bin/rm -f
MV = /bin/mv
MKDIR = mkdir -p
PLATFORM = linux-x86-64

########################################################################
# New suffixes and associated building rules -- edit with care



MT_OBJECTS = MTeiffel_curl.o
OBJECTS = eiffel_curl.o 

ALL_OBJECTS = $(OBJECTS) $(MT_OBJECTS)

all: $(ALL_OBJECTS)
	$(MKDIR) ../spec/$(PLATFORM)/lib
	$(MV) $? ../spec/$(PLATFORM)/lib
	$(MAKE) clobber

MTeiffel_curl.o: eiffel_curl.c
	$(CC) -c $(MTCFLAGS) $? -o $@

########################################################################
# Common rules for all Makefiles -- do not edit

emptyrule::

clean: local_clean
realclean: local_realclean
clobber: local_clobber

local_clean::
	$(RM) core *~ *.o *.so *.a

local_realclean:: local_clean

local_clobber:: local_realclean
	$(RM) Makefile config.sh

Makefile: Makefile.SH
	/bin/sh Makefile.SH

tags::
	$(CTAGS) -w *.[ch]
	$(CTAGS) -xw *.[ch] > tags

local_clobber::
	$(RM) tags

